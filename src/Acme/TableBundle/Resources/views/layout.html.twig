<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <link rel="stylesheet" href="{{ asset('bundles/acmetable/css/demo.css') }}" type="text/css" media="all" />
        <title>{% block title %}Demo Bundle{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        <script type="text/javascript" src=" {{ asset('bundles/acmetable/js/jquery-1.6.1.js') }}"></script> 
        <script type="text/javascript" src=" {{ asset('bundles/acmetable/js/jquery.tablesorter.js') }}"></script> 
        <script type="text/javascript">
            $(document).ready(function() 
            { 
                $("#tablesorter-demo").tablesorter({sortList:[[0,0]], widgets: ['zebra']});
                $("#options").tablesorter({sortList: [[0,0]], headers: { 3:{sorter: false}, 4:{sorter: false}}});
            } 
            
            
        ); 
        </script>
        <link href="{{ asset('bundles/acmetable/css/style.css') }}" rel="stylesheet" type="text/css" />
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    </head>
    <body>
        <div id="symfony-wrapper">
            <div id="symfony-header">
            </div>

            {% if app.session.flash('notice') %}
            <div class="flash-message">
                <em>Notice</em>: {{ app.session.flash('notice') }}
            </div>
            {% endif %}

            {% block content_header %}
            <ul id="menu">
                    {% block content_header_more %}
                <li><a href="{{ path('homepage') }}">Home</a></li>
                    {% endblock %}
            </ul>

            <div style="clear: both"></div>
            {% endblock %}

            <div class="symfony-content">
                {% block content %}
                {% endblock %}
            </div>

            {% if code is defined %}
            <h2>Code behind this page</h2>
            <div class="symfony-content">{{ code|raw }}</div>
            {% endif %}
        </div>
    </body>
</html>
